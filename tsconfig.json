{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": false,
    "paths": {
      "@Config/*": [
        "./src/config/*"
      ],
      "@Resources/*": [
        "./src/resources/*"
      ],
      "@Helper/*": [
        "./src/helper/*"
      ],
      "@Services/*": [
        "./src/services/*"
      ],
      "@Types/*": [
        "./src/types/*"
      ],
      "@Schema/*": [
        "./src/schema/*"
      ],
      "@Validation/*": [
        "./src/validations/*"
      ],
      "@Static/*": [
        "./src/static/*"
      ]
    }
  }
}